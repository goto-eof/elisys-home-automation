<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="schemaName" value="public"/>
    <property name="tablePrefix" value="ha_"/>
    <property name="dbms" value="postgresql"/>
    <property name="autoIncrement" value="true" dbms="postgresql"/>
    <property name="defaultTimeStamp" value="current_timestamp" dbms="mysql, postgresql"/>

    <includeAll path="db/changelog/changesets"/>

</databaseChangeLog>